{
  "name": "@h6s/calendar-v2",
  "version": "0.0.1",
  "description": "TanStack-style headless calendar - PoC",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
